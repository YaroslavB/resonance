{
    "name": "distantmagic/resonance",
    "description": "PHP Hub Framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Distantmagic\\Docs\\": "app/",
            "Distantmagic\\Resonance\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mateusz Charytoniuk",
            "email": "mc@distantmagic.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-igbinary": "*",
        "distantmagic/graphql-swoole-promise-adapter": "^0.1.1",
        "distantmagic/swoole-futures": "^0.1.1",
        "doctrine/dbal": "^3.7",
        "doctrine/migrations": "^3.6",
        "doctrine/orm": "^2.16",
        "doctrine/sql-formatter": "^1.1",
        "ezyang/htmlpurifier": "^4.16",
        "league/commonmark": "^2.4",
        "nette/php-generator": "^4.1",
        "nette/schema": "^1.2",
        "nicmart/tree": "^0.7.2",
        "psr/container": "^2.0",
        "psr/link": "^2.0",
        "psr/log": "^3.0",
        "symfony/console": "^6.3",
        "symfony/event-dispatcher": "^6.3",
        "symfony/filesystem": "^6.3",
        "symfony/finder": "^6.3",
        "symfony/routing": "^6.3",
        "symfony/yaml": "^6.3",
        "twig/cache-extra": "^3.7",
        "twig/twig": "^3.7",
        "webonyx/graphql-php": "^15.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0",
        "swoole/ide-helper": "^5.1"
    },
    "suggest": {
        "ext-ds": "For better memory management",
        "ext-intl": "Date formatting",
        "ext-readline": "Unlocks some console features",
        "ext-redis": "HTTP Sessions driver",
        "ext-uuid": "Faster UUID generation"
    }
}
