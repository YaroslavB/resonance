{
    "name": "distantmagic/resonance",
    "description": "PHP SaaS Framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Distantmagic\\Docs\\": "app/",
            "Distantmagic\\Resonance\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mateusz Charytoniuk",
            "email": "mc@distantmagic.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "defuse/php-encryption": "^2.4",
        "distantmagic/graphql-swoole-promise-adapter": "^0.1.1",
        "distantmagic/swoole-futures": "^0.1.2",
        "doctrine/dbal": "^3.7",
        "doctrine/migrations": "^3.6",
        "doctrine/orm": "^2.16",
        "doctrine/sql-formatter": "^1.1",
        "ezyang/htmlpurifier": "^4.16",
        "guzzlehttp/guzzle": "^7.8",
        "league/commonmark": "^2.4",
        "league/oauth2-server": "^8.5",
        "nette/php-generator": "^4.1",
        "nicmart/tree": "^0.7.2",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "psr/container": "^2.0",
        "psr/http-message": "^2.0",
        "psr/link": "^2.0",
        "psr/log": "^3.0",
        "symfony/console": "^7.0",
        "symfony/event-dispatcher": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/finder": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/yaml": "^7.0",
        "twig/cache-extra": "^3.7",
        "twig/twig": "^3.7",
        "webonyx/graphql-php": "^15.6",
        "dragonmantank/cron-expression": "^3.3",
        "league/oauth2-client": "^2.7",
        "opis/json-schema": "^2.3",
        "symfony/mailer": "^7.0",
        "symfony/messenger": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0",
        "swoole/ide-helper": "^5.1"
    },
    "suggest": {
        "ext-ds": "For better memory management",
        "ext-intl": "Date formatting",
        "ext-igbinary": "For better object serialization",
        "ext-openssl": "To generate application keys",
        "ext-readline": "Unlocks some console features",
        "ext-redis": "HTTP Sessions driver",
        "ext-uuid": "Faster UUID generation"
    }
}
